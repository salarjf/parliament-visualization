  script.
    var sid=#{namayadneDetails.S_ID};
    var totaltazakor=#{namayadneDetails.total_tazakor}
    var totalsoal=#{namayadneDetails.total_soal}
    var totaltarh=#{namayadneDetails.total_tarh}
    var totalnotgh=#{namayadneDetails.total_notgh}
    var totaladam=#{namayadneDetails.adam}
    var ostanname="#{namayadneDetails.ostan}"
    var nid="9"+sid;
    var adam=#{namayadneDetails.adam};
    $(drawTazakor(nid));
    $(drawSoal(nid));
    $(drawMosharekat(nid));
    $(drawTazakorHistogram(totaltazakor));
    $(drawNotghHistogram(totalnotgh));
    $(drawAdamHistogram(adam));
    $(drawMosharekatHistogram(totaltarh));
    $(drawSoalHistogram(totalsoal));
    var kolesoalat=3400;
    var kolletazakorat=16124;
    var koletarha=16643;
    var kolenotghha=1142;
    var kolezamanejalasat=95423;
    $(setDarsadSoal(((totalsoal*100/kolesoalat)).toString().substring(0,3)))
    $(setDarsadTazakor(((totaltazakor*100/kolletazakorat)).toString().substring(0,3)))
    $(setDarsadMosharekat(totaltarh,((totaltarh*100/koletarha)).toString().substring(0,3)),'xxxx')
    $(setDarsadNotgh(totalnotgh*100/kolenotghha))
    $(setDarsadAdam(totaladam*100/kolezamanejalasat))

    $(drawAllWordClouds(nid))




    $(buildComisionTable(nid))
    $(drawMap(ostanname))



