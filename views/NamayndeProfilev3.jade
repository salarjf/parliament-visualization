extends layout
block profilev3
  .row
    .col-md-8
      .widget
        .widget-advanced
          // Widget Header
          .widget-header.text-center.themed-background
            h2.widget-content-light(style='font-size: 20px; color:gray;')
              strong اطلاعات کمیسیون
          // END Widget Header
          // Widget Main
          .widget-main.profilewidget.comisionwidget
            .table-responsive
              table#comision-table.table.table-vcenter.table-condensed.table-bordered.table-striped
                thead
                  tr
                    th(style="text-align:center;") سال چهارم
                    th(style="text-align:center;") سال سوم
                    th(style="text-align:center;") سال دوم
                    th(style="text-align:center;") سال اول
                    th(style="text-align:center;") 
                tbody(id="comisiontablebody")

    .col-md-4
      .widget
        .widget-advanced
          // Widget Header
          .widget-header.text-center.themed-background
            h2.widget-content-light
              strong #{namayadneDetails.naam}
          // END Widget Header
          // Widget Main
          .widget-main.profilewidget
            img.widget-image-container(alt='Namayande Pic', src='/njsstatic/images/namayandegan/#{namayadneDetails.S_ID}.png')
            .gallery.gallery-widget(data-toggle='lightbox-gallery')
              .row
                .col-md-7.tahsilat
                  .widget-simple
                    h3.widget-content.text-right.animation-pullDown
                      strong تحصیلات
                      small #{namayadneDetails.tahsilat}
                .col-md-3.birthplace
                  .widget-simple
                    h3.widget-content.text-right.animation-pullDown
                      strong محل تولد
                      small #{namayadneDetails.makanetavalod}   
                .col-md-2.age
                  .widget-simple
                    h3.widget-content.text-right.animation-pullDown
                      strong سن
                      small #{namayadneDetails.sen} 
  .block.full
    // Working Tabs Title
    .block-title
      h2(id=("faliatshout"))
        |    فعالیت های نماینده    
        small در دوره نهم
    // END Working Tabs Title
    // Working Tabs Content
    ul.nav.nav-tabs(data-toggle='tabs')
      li
        a(href='#rahyabi') اطلاعات آراء
      li.active
        a(href='#huzurghiab') نظم حضور در جلسات
      li
        a(href='#mosharekat') طرح های امضا شده 
      li
        a(href='#notgh') نطق های پیش از دستور
      li
        a(href='#tazakor') تذکرات کتبی به وزرا
      li
        a(href='#soal') سوال از وزرا در کمیسیون ها

        // Default Tabs
    .tab-content
      .tab-pane(id='rahyabi')
        .row.upperrow
          .col-md-6.chartcol
            a.widget.widget-hover-effect2
              .widget-simple.themed-background-modern
                h4.widget-content.widget-content-light.text-center
                  strong نقشه استان حوزه انتخابیه
              .widget-extra.datapiecharts
                #iranmap
          .col-md-6
            .chartcol.nearmap
              a.widget.widget-hover-effect2
                .widget-simple.themed-background-modern
                  h1.widget-content.widget-content-light.text-center
                    strong حوزه انتخابیه
                .widget-extra.datapiecharts.half
                  .widget-simple
                    .shouthoze  
                      |#{namayadneDetails.hoze}
                    .shouthoze.ostanesh
                      |استان #{namayadneDetails.ostan}
            .chartcol.nearmap
              a.widget.widget-hover-effect2
                .widget-simple.themed-background-modern
                  h4.widget-content.widget-content-light.text-center
                    strong اطلاعات آراء
                .widget-extra.datapiecharts.half
                  .widget-simple
                    .row
                      .col-md-6
                        .shoutray
                          |درصد رای  
                        .shouthoze.ostanesh
